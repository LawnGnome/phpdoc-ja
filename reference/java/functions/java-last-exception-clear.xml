<refentry xml:id="function.java-last-exception-clear" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>java_last_exception_clear</refname>
  <refpurpose>直近の例外をクリアする</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>java_last_exception_clear</methodname>
