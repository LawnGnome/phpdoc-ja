<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagickdraw-circle">
 <refnamediv>
  <refname>ImagickDraw::circle</refname>
  <refpurpose>円を描画する</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ImagickDraw::circle</methodname>
   <methodparam><type>float</type><parameter>ox</parameter></methodparam>
   <methodparam><type>float</type><parameter>oy</parameter></methodparam>
   <methodparam><type>float</type><parameter>px</parameter></methodparam>
   <methodparam><type>float</type><parameter>py</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   画像上に円を描画します。
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>ox</parameter></term>
     <listitem>
      <para>
       原点の x 座標。
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>oy</parameter></term>
     <listitem>
      <para>
       原点の y 座標。
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>px</parameter></term>
     <listitem>
      <para>
       周囲の x 座標。
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>py</parameter></term>
     <listitem>
      <para>
       周囲の y 座標。
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>

