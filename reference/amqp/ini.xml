      <entry>PHP_INI_ALL</entry>
      <entry><!-- leave empty, this will be filled by an automatic script --></entry>
     </row>
     <row>
      <entry><link linkend="ini.amqp.min-messages">amqp.min_messages</link></entry>
      <entry>0</entry>
      <entry>PHP_INI_ALL</entry>
      <entry><!-- leave empty, this will be filled by an automatic script --></entry>
     </row>
