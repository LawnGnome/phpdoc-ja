<?xml version="1.0" encoding="utf-8"?>
<!-- splitted from ./ja/functions/ftp.xml, last change in rev 1.1 -->
<!-- sync: 1.2 -->
  <refentry id="function.ftp-rmdir">
   <refnamediv>
    <refname>ftp_rmdir</refname>
    <refpurpose>ディレクトリを削除する</refpurpose>
   </refnamediv>
   <refsect1>
    <title>説明</title>
     <methodsynopsis>
      <type>bool</type><methodname>ftp_rmdir</methodname>
      <methodparam><type>resource</type><parameter>ftp_stream</parameter></methodparam>
      <methodparam><type>string</type><parameter>directory</parameter></methodparam>
     </methodsynopsis>
    <para> 
     指定した<parameter>directory</parameter>を削除します。
    </para>
    <para>
     成功時に &true;、エラー時に &false; を返します。
    </para>
   </refsect1>
  </refentry>

