 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>http_persistent_handles_count</function> の例</title>
    <programlisting role="php">
<![CDATA[
<?php
print_r(http_persistent_handles_count());
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
stdClass Object
(
    [http_request] => Array
        (
            [GLOBAL] => Array
                (
                    [used] => 0
                    [free] => 1
                )

        )

    [http_request_datashare] => Array
        (
            [GLOBAL] => Array
                (
                    [used] => 1
                    [free] => 0
                )

        )

    [http_request_pool] => Array
        (
        )

)
]]>
