<refentry xml:id="function.unicode-semantics" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>unicode_semantics</refname>
  <refpurpose>unicode 機能が使用可能かどうかを調べる</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>unicode_semantics</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>unicode_semantics</function> は、
   unicode 機能が使用可能かどうかを調べます。
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; あるいは &false; を返します。
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  &warn.experimental.func;
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>unicode_encode</function></member>
    <member><function>unicode_decode</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

