<!-- splitted from ./ja/functions/ovrimos.xml, last change in rev 1.1 -->
<!-- sync: 1.1 -->
  <refentry id="function.ovrimos-result">
   <refnamediv>
    <refname>ovrimos_result</refname>
    <refpurpose>出力カラムを取得する</refpurpose>
   </refnamediv>
   <refsect1>
    <title>説明</title>
     <methodsynopsis>
      <type>int</type><methodname>ovrimos_result</methodname>
      <methodparam><type>int</type><parameter>result_id</parameter></methodparam>
      <methodparam><type>mixed</type><parameter>field</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>ovrimos_result</function> は出力カラムを取得するために
     使用されます。
    </para>
    <para>
     <function>ovrimos_result</function> は 'field' に文字列または、1
     から始まる番号で指定した出力カラムを取得します。
    </para>
    </refsect1>
  </refentry>

